package edu.gyansetu.tcs.datastructures;

public class MyLinkedList {

	private MyNode start;

	class MyNode {
		MyNode prev;
		Object data;
		MyNode next;

		public MyNode(Object data) {
			this.data = data;
		}

	}

	public MyLinkedList() {
		start = new MyNode(null);
	}

	public void add(Object data) {
		MyNode myNode = new MyNode(data);
		start.next = myNode;
	}

}
