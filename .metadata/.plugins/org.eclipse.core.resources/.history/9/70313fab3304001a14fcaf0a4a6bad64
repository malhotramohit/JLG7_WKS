
public class Array2 {

	public static void main(String[] args) {

		int[] array1 = { 23, -89, 67, 45, 34, 20, -69 };

		int max = array1[0];

		for (int i = 1; i < array1.length; i++) {

			if (max < array1[i]) {
				max = array1[i];
			}

		}

		System.out.println("max is " + max);

		// 23, -89, 67, 45, 34, 20

		// -89 23 67 45 34 20
		// -89 23 67 45 34 20
		// -89 23 45 67 34 20
		// -89 23 45 34 67 20
		// -89 23 45 34 20 67

		// -89 23 45 34 20 67

	}
}
