package edu.gyansetu.tcs.collections;

import java.util.ArrayList;
import java.util.Collections;

public class TestArrayList5 {

	public static void main(String[] args) {

		Wallet wallet1 = new Wallet(12, 233, "B1");
		Wallet wallet2 = new Wallet(14, 2336, "B2");

		Wallet wallet3 = new Wallet(9, 898, "A1");

		Wallet wallet4 = new Wallet(10, 21000, "C1");
		Wallet wallet5 = new Wallet(16, 5666, "H1");

		ArrayList<Wallet> wallets = new ArrayList<Wallet>(5);

		wallets.add(wallet1);
		wallets.add(wallet2);
		wallets.add(wallet3);
		wallets.add(wallet4);
		wallets.add(wallet5);
		
		System.out.println("before sorting");
		wallets.forEach(System.out::println);

		Collections.sort(wallets);
		
		
		
		
		// 128 3 34 67 13
		// 3 128 34 67 13
		// 3 34 128 67 13
		// 3 34 67 128 13
		// 3 34 67 13 128

		// 3 34 67 13 128

	}
}
