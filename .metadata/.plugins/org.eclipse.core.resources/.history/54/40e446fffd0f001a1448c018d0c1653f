package edu.gyansetu.tcs.oops.revision.baisc;

public class Laptop {

	int price; // instance var
	String type;

	public static void main(String[] args) {

		System.out.println("hello");

		// obj ref variable
		Laptop mac = new Laptop();

		mac.price = 23;
		mac.type = "i8";

		Laptop hp = new Laptop();

		hp.price = 89;
		hp.type = "i5";
		
		
		displayResult(hp);

	}
	
	public static void displayResult(Laptop laptop) {
		System.out.print(laptop.price+" ");
		System.out.println(laptop.type);
	}

}
