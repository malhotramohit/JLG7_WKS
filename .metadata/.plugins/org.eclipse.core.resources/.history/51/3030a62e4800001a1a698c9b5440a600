
public class Loops2 {

	public static void main(String[] args) {

		for (int i = 1; i < 5; i++) {

			if (i % 3 == 0) {
				break;
			}
			System.out.println("val is " + i);
		}

		// *****
		// *****
		// *****
		// *****

		for (int i = 0; i < 4; i++) {
			
			
		}

		for (int i = 0; i < 5; i++) {
			System.out.print("*");
		}
		System.out.println();

		for (int i = 0; i < 5; i++) {
			System.out.print("*");
		}
		System.out.println();

		for (int i = 0; i < 5; i++) {
			System.out.print("*");
		}
	}

}
